<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id = "dog" class="com.pknu.hello.di.Dog">
		<property name="name" value ="바우바우"/>
	</bean>
		
	<bean id="cat" class="com.pknu.hello.di.Cat">
		<property name="name" value="애옹이"/>
	</bean>
	
	<bean id="petOwner" class="com.pknu.hello.di.PetOwner">
		<constructor-arg ref = "dog"></constructor-arg>
	</bean>
</beans>
